@model IEnumerable<prjHouseWebAp.Models.地點名稱>

@{
    ViewBag.Title = "房屋資訊";
}

<h2>房屋資訊</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.地點編號)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.類型)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.帳號)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.名稱)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.說明)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.地址)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.照片)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.地點編號)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.類型)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.帳號)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.名稱)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.說明)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.地址)
        </td>
        <td>
            <img src="~/Images/@item.照片" width="150" />
        </td>
    </tr>
}

</table>
